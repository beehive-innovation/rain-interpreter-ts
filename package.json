{
    "name": "@beehiveinnovation/rain-interpreter-ts",
    "version": "0.0.0",
    "description": "Rain Interpreter written in TypeScript",
    "author": "Beehive Innovation",
    "license": "CAL-1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/beehive-innovation/rain-interpreter-ts.git"
    },
    "keywords": [],
    "bugs": {
        "url": "https://github.com/beehive-innovation/rain-interpreter-ts/issues"
    },
    "homepage": "http://beehive-innovation.github.io/rain-interpreter-ts/",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist/index.js",
    "files": [
        "dist",
        "src",
        "docs"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "build": "tsc",
        "prepare": "tsc",
        "test": "npx hardhat test",
        "lint": "eslint \"src/**\" \"test/**\"",
        "docgen": "api-extractor run --local && api-documenter -i ./ -o ./docs",
        "copy-contracts": "mkdir -p contracts && cp -r node_modules/@beehiveinnovation/rain-protocol/contracts . && hardhat compile --no-typechain",
        "gen-typechain": "hardhat typechain && rm -rf src/typechain && mkdir -p src/typechain && cp -r typechain src"
    },
    "prettier": {
        "printWidth": 100,
        "singleQuote": true,
        "semi": false,
        "tabWidth": 4,
        "useTabs": false,
        "trailingComma": "es5"
    },
    "devDependencies": {
        "@beehiveinnovation/rain-protocol": "git+https://github.com/beehive-innovation/rain-protocol.git#f40c1589fafd17ab07146c6d68ae42ba3b3c53de",
        "@microsoft/api-extractor": "^7.33.5",
        "@nomiclabs/hardhat-ethers": "2.1.1",
        "@nomiclabs/hardhat-waffle": "2.0.1",
        "@princjef/api-documenter": "^1.0.2",
        "@typechain/ethers-v5": "10.1.0",
        "@typechain/hardhat": "6.1.2",
        "@types/chai": "^4.3.3",
        "@types/mocha": "^10.0.0",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "chai": "^4.3.6",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "hardhat": "2.10.2",
        "hardhat-contract-sizer": "2.1.1",
        "hardhat-gas-reporter": "1.0.4",
        "hardhat-storage-layout": "0.1.6",
        "mocha": "^10.1.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.1",
        "typechain": "8.1.0",
        "typescript": "^4.8.4"
    },
    "dependencies": {
        "ethers": "^5.7.2",
        "prettier": "^2.7.1"
    }
}
